{
  "dependencies": {
    "cypress": "^15.2.0",
    "mochawesome": "^7.1.4",
    "mochawesome-report-generator": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2",
    "jest": "^30.1.3",
    "jest-summary-reporter": "^0.0.2",
    "mochawesome-merge": "^4.2.0",
    "marge": "^1.0.1",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "pretest": "npx rimraf cypress/report/mochawesome-report",
    "test": "cypress run --reporter mochawesome",
    "posttest": "npx mochawesome-merge cypress/report/mochawesome-report/*.json > cypress/report/output.json && npx marge cypress/report/output.json --reportDir ./ --inline"
  }
}
